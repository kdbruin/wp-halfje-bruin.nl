{
    "name": "halfje-bruin/halfje-bruin",
    "description": "Halfje-Bruin WordPress site",
    "authors": [
        {
            "name": "Kees de Bruin",
            "homepage": "https://www.halfje-bruin.nl/"
        }
    ],
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
          "type": "vcs",
          "url": "git@git.home.lan:base/hb-plugin.git"
        }
    ],
    "config": {
        "vendor-dir": "vendor"
    },
    "require": {
        "vlucas/phpdotenv": "^1.0.6",
        "johnpbloch/wordpress": ">=4.9",
        "rarst/update-blocker": "^1.1",
        "kdb/hb-plugin": "dev-master",    
        "wpackagist-plugin/advanced-custom-fields": "^4.4.12",
        "wpackagist-plugin/advanced-gutenberg": "^1.6.1",
        "wpackagist-plugin/atomic-blocks": "^1.3.3",
        "wpackagist-plugin/backupwordpress": "^3.6.4",
        "wpackagist-plugin/classic-smilies": "^1.3",
        "wpackagist-plugin/crayon-syntax-highlighter": "^2.8.4",
        "wpackagist-plugin/disqus-comment-system": "^3.0.15",
        "wpackagist-plugin/enable-media-replace": "^3.2.5",
        "wpackagist-plugin/force-regenerate-thumbnails": "^2.0.6",
        "wpackagist-plugin/gutenberg": "^3.5.0",
        "wpackagist-plugin/post-snippets": "^3.0.5",
        "wpackagist-plugin/ricg-responsive-images": "^3.1.1",
        "wpackagist-plugin/series": "^2.0.0",
        "wpackagist-plugin/shortcodes-ultimate": "^5.0.3",
        "wpackagist-theme/atomic-blocks": "^1.1.9",
        "wpackagist-theme/gridsby": "^1.2.9"
    },
    "require-dev": {
        "rarst/laps": "^1.4.4",
        "wpackagist-plugin/a-fresher-cache": "*",
        "wpackagist-plugin/core-control": "*",
        "wpackagist-plugin/loco-translate": "^2.1.3",
        "wpackagist-plugin/monster-widget": "*",
        "wpackagist-plugin/php-compatibility-checker": "*",
        "wpackagist-plugin/theme-check": "*",
        "wpackagist-plugin/wordpress-importer": "*"
    },
    "extra": {
        "wordpress-install-dir": "wp",
        "installer-paths": {
            "app/plugins/{$name}/": [
                "type:wordpress-plugin"
            ],
            "app/mu-plugins/{$name}/": [
                "type:wordpress-muplugin"
            ],
            "app/themes/{$name}/": [
                "type:wordpress-theme"
            ]
        }
    }
}